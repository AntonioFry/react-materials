{
  "entry": "src/index.jsx",
  "publicPath": "./",
  "alias": {
    "@": "./src/"
  },
  "devServer": {
    "historyApiFallback": true
  },
  "plugins": [
    "build-plugin-ice-app",
    ["build-plugin-fusion", {
      "themePackage": "@icedesign/theme"
    }],
    ["build-plugin-moment-locales", {
      "locales": ["zh-cn"]
    }]
  ]
}
